{
    "name": "php-libraries/repository",
    "type": "library",
    "description": "In Process/Request Data Repository",
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.dealnews.net/"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "6.5.*"
    },
    "require": {
        "php": ">=7.1.0",
    },
    "autoload": {
        "psr-4": {
            "DealNews\\Repository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DealNews\\Repository\\Tests\\": "tests/"
        }
    }
}